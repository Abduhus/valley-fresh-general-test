{
  "name": "valley-preview-perfume-ecommerce",
  "version": "1.0.0",
  "description": "Luxury Perfume E-commerce Platform",
  "main": "server/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "npm run build:server && npm run build:client",
    "build:server": "npx tsc --project tsconfig.build.json",
    "build:client": "vite build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "preview": "vite preview",
    "check": "npx tsc --noEmit",
    "db:push": "drizzle-kit push:pg",
    "deploy": "npm run start",
    "deploy:setup": "powershell -ExecutionPolicy Bypass -File setup-deployment.ps1",
    "start:windows": "start.bat",
    "railway:deploy": "railway up",
    "railway:logs": "railway logs",
    "build:docker": "docker build -t valley-preview .",
    "render:build": "npm run build",
    "render:start": "npm run start"
  },
  "keywords": [
    "perfume",
    "ecommerce",
    "luxury",
    "fragrance",
    "react",
    "typescript",
    "express"
  ],
  "author": "Valley Breezes",
